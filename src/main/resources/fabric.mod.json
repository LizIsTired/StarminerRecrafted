{
  "schemaVersion": 1,
  "id": "starminerrecrafted",
  "version": "${version}",
  "name": "Starminer Recrafted",
  "description": "A modern Fabric rewrite of the popular Forge 1.7.10 mod StarMiner",
  "authors": [
    "LizIsTired"
  ],
  "contact": {
    "issues": "https://github.com/LizIsTired/StarminerRecrafted/issues",
    "sources": "https://github.com/LizIsTired/StarminerRecrafted"
  },
  "license": "AGPL-3.0",
  "icon": "assets/starminerrecrafted/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      "net.lizistired.starminerrecrafted.MainInit"
    ],
    "client": [
      "net.lizistired.starminerrecrafted.MainInitClient"
    ]
  },
  "mixins": [
    "starminerrecrafted.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17",
    "gravitychanger": "^0.3.0"
  },
  "suggests": {
    "clothconfig": ">=6.1.48",
    "modmenu": ">=3.0.1"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/AgBfPFMJgX"
      }
    }
